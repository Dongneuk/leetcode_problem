class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        
        nums1 = set(nums1)
        nums2 = set(nums2)
        
        return list(nums1 & nums2)
    
## time complexity = O(N+M) where N = len(nums1), M = len(nums2).
        